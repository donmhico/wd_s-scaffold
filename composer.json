{
    "name": "donmhico/wd_s-scaffold",
    "type": "wp-cli-package",
    "description": "Generates a new theme based on wd_s.",
    "authors": [
        {
            "name": "Michael Joseph Panaga",
            "email": "michael.panaga@webdevstudios.com"
        }
    ],
    "require": {
        "wp-cli/wp-cli": "^2.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "bundled": true,
        "commands": [
            "scaffold wd_s"
        ]
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "command.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
